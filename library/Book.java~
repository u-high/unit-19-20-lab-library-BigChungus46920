package library;

public class Book{
  
  String author;
  String title;
  int isbn;
  
  public Book(){
   this.Book("author","title",-1);
  }
  public Book(String a, String b, int c){
    author = a;
    title = b;
    isbn = c;
  }
  
  public void setAuthor(String a){
    author = a;
  }
  public void setTitle(String a){
    title = a;
  }
  public void setISBN(int a){
    isbn=a;
  }
  public String getAuthor(){
    return author;
  }
  public String getTitle(){
    reutnr title;
  }
  public int getISBN(){
    return isbn;
  }
  
  public String toString(){
    return "Author: "+ author+"\nTitle: "+title+"\n ISBN: " + isbn;
  }
}
  
  